{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "author": "Hugor",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.2.2",
    "node-sass": "^4.8.3",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "font-awesome": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inhugor/confusion.git"
  },
  "bugs": {
    "url": "https://github.com/Inhugor/confusion/issues"
  },
  "homepage": "https://github.com/Inhugor/confusion#readme"
}
